#include <stdio.h>
int main() 
{
    int n,i,sp,j,a=1;
    printf("Enter no of rows:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(sp=1;sp<=n-i;sp++)
        {
            printf("\t");
        }
        for(j=1;j<=i;j++)
        {
            printf("%d\t\t",a++);
        }printf("\n");
    }
    return 0;
}
