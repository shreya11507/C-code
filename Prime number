#include <stdio.h>
int main() 
{
   int num,i,count=0;
   printf("Enter number:");
   scanf("%d",&num);
   for(i=2;i<num/2;i++)
   {
       if(num%i != 0)
      continue;
       else
       {
       count=1;
       printf("%d is a Prime number",num);
       break;
       }
   }
   if(count == 0)
   printf("%d is not a primenumber",num);
   return 0;
}
