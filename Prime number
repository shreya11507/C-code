#include <stdio.h>
int main() 
{
   int num,i;
   printf("Enter number:");
   scanf("%d",&num);
   for(i=2;i<num;i++)
   {
       if(num%i == 0)
       {
         printf("%d is a Composite number",num);
         break;
       }
       else
       printf("%d is a Prime number",num);
       break;
   }

    return 0;
}
